proc vTclWindow. {base} {
    if {$base == ""} {
        set base .
    }
    ###################
    # CREATING WIDGETS
    ###################
    wm focusmodel $top passive
    wm geometry $top 200x200+22+22; update
    wm maxsize $top 1284 785
    wm minsize $top 104 1
    wm overrideredirect $top 0
    wm resizable $top 1 1
    wm withdraw $top
    wm title $top "vtcl"
    bindtags $top "$top Vtcl all"
    vTcl:FireEvent $top <<Create>>
    wm protocol $top WM_DELETE_WINDOW "vTcl:FireEvent $top <<>>"

    ###################
    # SETTING GEOMETRY
    ###################

    vTcl:FireEvent $base <<Ready>>
}

proc vTclWindow.top999 {base} {
    if {$base == ""} {
        set base .top999
    }
    if {[winfo exists $base]} {
        wm deiconify $base; return
    }
    set top $base
    ###################
    # CREATING WIDGETS
    ###################
    vTcl:toplevel $top -class Toplevel
    wm withdraw $top
    wm focusmodel $top passive
    wm geometry $top 400x130+200+100; update
    wm maxsize $top 1604 1184
    wm minsize $top 113 1
    wm overrideredirect $top 0
    wm resizable $top 1 1
    wm title $top "Track Variable"
    vTcl:DefineAlias "$top" "Toplevel999" vTcl:Toplevel:WidgetProc "" 1
    bindtags $top "$top Toplevel all _TopLevel"
    vTcl:FireEvent $top <<Create>>
    wm protocol $top WM_DELETE_WINDOW "vTcl:FireEvent $top <<>>"

    frame $top.fra69 \
        -borderwidth 2 -height 75 -width 125 
    vTcl:DefineAlias "$top.fra69" "Frame1" vTcl:WidgetProc "Toplevel999" 1
    set site_3_0 $top.fra69
    button $site_3_0.cpd70 \
        -background #ffff00 \
        -command {Window hide $widget(Toplevel999); TextEditorRunTrace "Close Window Track Variable" "b"} \
        -padx 4 -pady 2 -text Exit 
    bindtags $site_3_0.cpd70 "$site_3_0.cpd70 Button $top all _vTclBalloon"
    bind $site_3_0.cpd70 <<SetBalloon>> {
        set ::vTcl::balloon::%W {Exit the Function}
    }
    pack $site_3_0.cpd70 \
        -in $site_3_0 -anchor center -expand 1 -fill none -side left 
    TitleFrame $top.tit92 \
        -ipad 0 -text {Variable Name} 
    vTcl:DefineAlias "$top.tit92" "TitleFrame1" vTcl:WidgetProc "Toplevel999" 1
    bind $top.tit92 <Destroy> {
        Widget::destroy %W; rename %W {}
    }
    set site_4_0 [$top.tit92 getframe]
    entry $site_4_0.cpd79 \
        -background #ffffff -disabledbackground #ffffff \
        -disabledforeground #ff0000 -foreground #ff0000 \
        -textvariable VariableName 
    vTcl:DefineAlias "$site_4_0.cpd79" "Entrytop999" vTcl:WidgetProc "Toplevel999" 1
    frame $site_4_0.fra81 \
        -borderwidth 2 -height 75 -width 125 
    vTcl:DefineAlias "$site_4_0.fra81" "Frame5" vTcl:WidgetProc "Toplevel999" 1
    set site_5_0 $site_4_0.fra81
    button $site_5_0.but72 \
        -background #ffff00 \
        -command {set VariableValue ??
    set VariableValue [expr $$VariableName]} \
        -padx 4 -pady 2 -text OK 
    vTcl:DefineAlias "$site_5_0.but72" "Button1" vTcl:WidgetProc "Toplevel999" 1
    pack $site_5_0.but72 \
        -in $site_5_0 -anchor center -expand 0 -fill none -side top 
    pack $site_4_0.cpd79 \
        -in $site_4_0 -anchor center -expand 1 -fill x -side left 
    pack $site_4_0.fra81 \
        -in $site_4_0 -anchor center -expand 0 -fill none -side right 
    TitleFrame $top.cpd71 \
        -ipad 0 -text {Variable Value} 
    vTcl:DefineAlias "$top.cpd71" "TitleFrame2" vTcl:WidgetProc "Toplevel999" 1
    bind $top.cpd71 <Destroy> {
        Widget::destroy %W; rename %W {}
    }
    set site_4_0 [$top.cpd71 getframe]
    entry $site_4_0.cpd79 \
        -background #ffffff -disabledbackground #ffffff \
        -disabledforeground #0000ff -foreground #0000ff -state disabled \
        -textvariable VariableValue 
    vTcl:DefineAlias "$site_4_0.cpd79" "EntryTop32" vTcl:WidgetProc "Toplevel999" 1
    pack $site_4_0.cpd79 \
        -in $site_4_0 -anchor center -expand 1 -fill x -side left 
    ###################
    # SETTING GEOMETRY
    ###################
    pack $top.fra69 \
        -in $top -anchor center -expand 0 -fill x -side bottom 
    pack $top.tit92 \
        -in $top -anchor n -expand 1 -fill x -side top 
    pack $top.cpd71 \
        -in $top -anchor center -expand 1 -fill x -side top 

    vTcl:FireEvent $base <<Ready>>
}

#############################################################################
## Binding tag:  _TopLevel

bind "_TopLevel" <<Create>> {
    if {![info exists _topcount]} {set _topcount 0}; incr _topcount
}
bind "_TopLevel" <<DeleteWindow>> {
    if {[set ::%W::_modal]} {
                vTcl:Toplevel:WidgetProc %W endmodal
            } else {
                destroy %W; if {$_topcount == 0} {exit}
            }
}
bind "_TopLevel" <Destroy> {
    if {[winfo toplevel %W] == "%W"} {incr _topcount -1}
}
