        if {$PolType == "pp2" } {
            set datafile $fichpathimg; append datafile "IMAGE_VV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".cos"; puts $ftmp $datafile
            set datafile $fichpathql; append datafile "QL_VV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".tif"; puts $ftmp $datafile
            for {set i 1} {$i <= 4} {incr i} { if {$polLayerCal($i) == "VV" } { puts $ftmp $calFactor($i) } }
            
            set datafile $fichpathimg; append datafile "IMAGE_VH_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".cos"; puts $ftmp $datafile
            set datafile $fichpathql; append datafile "QL_VH_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".tif"; puts $ftmp $datafile
            for {set i 1} {$i <= 4} {incr i} { if {$polLayerCal($i) == "VH" } { puts $ftmp $calFactor($i) } }
            }
        if {$PolType == "pp3" } {
            set datafile $fichpathimg; append datafile "IMAGE_HH_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".cos"; puts $ftmp $datafile
            set datafile $fichpathql; append datafile "QL_HH_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".tif"; puts $ftmp $datafile
            for {set i 1} {$i <= 4} {incr i} { if {$polLayerCal($i) == "HH" } { puts $ftmp $calFactor($i) } }
            
            set datafile $fichpathimg; append datafile "IMAGE_VV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".cos"; puts $ftmp $datafile
            set datafile $fichpathql; append datafile "QL_VV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".tif"; puts $ftmp $datafile
            for {set i 1} {$i <= 4} {incr i} { if {$polLayerCal($i) == "VV" } { puts $ftmp $calFactor($i) } }
            }
        if {$PolType == "pp0" } {
            set datafile $fichpathimg; append datafile "IMAGE_HV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".cos"; puts $ftmp $datafile
            set datafile $fichpathql; append datafile "QL_HV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".tif"; puts $ftmp $datafile
            for {set i 1} {$i <= 4} {incr i} { if {$polLayerCal($i) == "HV" } { puts $ftmp $calFactor($i) } }
            
            set datafile $fichpathimg; append datafile "IMAGE_VH_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".cos"; puts $ftmp $datafile
            set datafile $fichpathql; append datafile "QL_VH_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".tif"; puts $ftmp $datafile
            for {set i 1} {$i <= 4} {incr i} { if {$polLayerCal($i) == "VH" } { puts $ftmp $calFactor($i) } }
            }
        }

    if {$polarisationMode == "quad" } {
        puts $ftmp "full"

        set datafile $fichpathimg; append datafile "IMAGE_HH_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".cos"; puts $ftmp $datafile
        set datafile $fichpathql; append datafile "QL_HH_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".tif"; puts $ftmp $datafile
        for {set i 1} {$i <= 4} {incr i} { if {$polLayerCal($i) == "HH" } { puts $ftmp $calFactor($i) } }

        set datafile $fichpathimg; append datafile "IMAGE_HV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".cos"; puts $ftmp $datafile
        set datafile $fichpathql; append datafile "QL_HV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".tif"; puts $ftmp $datafile
