## Procedure:  load_bmp_file_new

proc ::load_bmp_file_new {bmpfile} {
global DataDir FileName BMPChange BMPImageOpen BMPDirInput BMPViewFileInput SourceWidth SourceHeight 
global BMPWidth BMPHeight WidthBMP HeightBMP ZoomBMP ZoomBMPAff
global BMPImage BMPImageLens BMPLens
global ImageSource BMPView BMPCanvas BMPFrame BMPSampleTab BMPSampleAff BMPSampleInd BMPZoomTab BMPZoomAff BMPZoomInd
global ColorNumber ColorNumberUtil ColorMapBMP RedPalette GreenPalette BluePalette
global RectLensCenter BMPWidthSource BMPHeightSource BMPSampleSource ZoomBMPSource
global BMPColorMapDisplay BMPColorMapGrayJetHsv BMPColorMapRedGreenBlue
global FlagBMPDisplay
global TMPBmpTmpHeader TMPBmpTmpData TMPBmp24TmpData TMPBmpTmpColormap TMPBmpColorBar TMPColorMapBMP TMPBmpDispTmp

image create photo BMPImage; BMPImage blank
image create photo BMPImageLens; BMPImageLens blank
image create photo BMPLens; BMPLens blank

WaitUntilCreated $TMPBmpTmpHeader
if [file exists $TMPBmpTmpHeader] {
    set f [open $TMPBmpTmpHeader r]
    gets $f SourceWidth
    gets $f SourceHeight
    close $f
    }

#show image
set ZoomBMP "0:0"; set ZoomBMPAff "0 %"
set subsample 0
if {$SourceWidth > $WidthBMP} {set subsample 1}
if {$SourceHeight > $HeightBMP} {set subsample 1}

set BMPView "VIEWBMPFILE"
set BMPCanvas "CANVASBMPFILE"
set BMPFrame "FRAMEBMPFILE"

set BMPSample 1
set BMPSampleInd 1
if {$subsample == 0} {
    set ZoomBMP "1:$BMPSampleTab($BMPSampleInd)"
    set ZoomBMPAff "$BMPSampleAff($BMPSampleInd) %"
    set FlagBMPDisplay "old"
    } else {
    if {$SourceWidth >= $SourceHeight} {
        while {[expr round($SourceWidth / $BMPSampleTab($BMPSampleInd))] > $WidthBMP} {incr BMPSampleInd}
        } else {
        while {[expr round($SourceHeight / $BMPSampleTab($BMPSampleInd))] > $HeightBMP} {incr BMPSampleInd}
        } 
    set ZoomBMP "1:$BMPSampleTab($BMPSampleInd)"
    set ZoomBMPAff "$BMPSampleAff($BMPSampleInd) %"
    set BMPSample $BMPSampleTab($BMPSampleInd)
    if {$BMPSample > 3} {
        set FlagBMPDisplay "new"
        set ProgressLine "0"
        update
        if {"$ColorNumber" != "BMP 24 Bits"} {
            TextEditorRunTrace "Process The Function Soft/bin/bmp_process/bmp_display.exe" "k"
            TextEditorRunTrace "Arguments: -ss $BMPSample -ifh \x22$TMPBmpTmpHeader\x22 -ifd \x22$TMPBmpTmpData\x22 -of \x22$TMPBmpDispTmp\x22 -ifc \x22$TMPBmpTmpColormap\x22" "k"
            set f [ open "| Soft/bin/bmp_process/bmp_display.exe -ss $BMPSample -ifh \x22$TMPBmpTmpHeader\x22 -ifd \x22$TMPBmpTmpData\x22 -of \x22$TMPBmpDispTmp\x22 -ifc \x22$TMPBmpTmpColormap\x22" r]
            }
        if {"$ColorNumber" == "BMP 24 Bits"} {
            TextEditorRunTrace "Process The Function Soft/bin/bmp_process/bmp24_display.exe" "k"
            TextEditorRunTrace "Arguments: -ss $BMPSample -ifh \x22$TMPBmpTmpHeader\x22 -ifd \x22$TMPBmp24TmpData\x22 -of \x22$TMPBmpDispTmp\x22" "k"
            set f [ open "| Soft/bin/bmp_process/bmp24_display.exe -ss $BMPSample -ifh \x22$TMPBmpTmpHeader\x22 -ifd \x22$TMPBmp24TmpData\x22 -of \x22$TMPBmpDispTmp\x22" r]
            }
        PsPprogressBar $f
        TextEditorRunTrace "Check RunTime Errors" "r"
        CheckRunTimeError
        } else {
        set FlagBMPDisplay "old"
        }
    } 

image delete ImageSource
if {$FlagBMPDisplay == "old"} {
    image create photo ImageSource -file $bmpfile
    if {$subsample == 0} {
        set BMPWidth $SourceWidth
        set BMPHeight $SourceHeight
        } else {
        set BMPWidth [expr round($SourceWidth / $BMPSample)]
        set BMPHeight [expr round($SourceHeight / $BMPSample)]
        }
    BMPImage copy ImageSource -from 0 0 $SourceWidth $SourceHeight -subsample $BMPSample $BMPSample
    }
if {$FlagBMPDisplay == "new"} {
    image create photo ImageSource -file $TMPBmpDispTmp
    set BMPWidth [image width ImageSource]
    set BMPHeight [image height ImageSource]
    #set SourceWidth $BMPWidth 
    #set SourceHeight $BMPHeight
    BMPImage copy ImageSource -from 0 0 $BMPWidth $BMPHeight -subsample 1 1
    }

set BMPSampleSource $BMPSample
set BMPWidthSource $BMPWidth
set BMPHeightSource $BMPHeight
set ZoomBMPSource $ZoomBMP
}
