    if {$TSXPol == "HV VH HV VH " } { set PolType "pp0" }
    if {$TSXPol == "VH HV VH HV " } { set PolType "pp0" }
    if {$TSXPol == "HH HV VH VV HH HV VH VV " } { set PolType "full" }
    puts $ftmp $PolType
    }

close $ftmp
}
