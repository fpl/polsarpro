            for {set i 1} {$i <= 4} {incr i} { if {$polLayerCal($i) == "HH" } { puts $ftmp $calFactor($i) } }
            
            set datafile $fichpathimg; append datafile "IMAGE_HV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".cos"; puts $ftmp $datafile
            set datafile $fichpathql; append datafile "QL_HV_SRA_"; append datafile $elevationBeamConfiguration; append datafile ".tif"; puts $ftmp $datafile
            for {set i 1} {$i <= 4} {incr i} { if {$polLayerCal($i) == "HV" } { puts $ftmp $calFactor($i) } }
