#############################################################################
## Procedure:  SNAPBatchProcess

proc ::SNAPBatchProcess {SNAPParameterFileInput SNAPParameterFileOutput} {
global SNAPDirInput SNAPDirOutput SNAPOutputDir SnapGpt
global SNAPLeaderFile SNAPSensor SNAPSensorPol PolarType
global SNAPResamplingDEM SNAPResamplingIMG SNAPPixelSize TMPSNAPConfigTxt
global SNAPMlkRgIn SNAPMlkAzIn SNAPMlkRgOut SNAPMlkAzOut SNAPDEM SNAPDEMFile SNAPRadioCorrec
global SNAPSaveDEM SNAPSaveIncAng SNAPSaveProjIncAng SNAPAntennaPass SNAPTeta0
global TMPSNAPBatchCfg TMPSNAPBatchOut TMPSNAPBatchRun TMPSNAPBatchDir TMPDir
global SnapReader PlatForm ImageConfig ConfigFile
global ConfigFileNameSearch ConfigFileNamePath ConfigFileNameVar VarConfigFileName 
global VarWarning WarningMessage WarningMessage2 VarError ErrorMessage
global ProgressLine FunctionAbort NligFullSize NcolFullSize 

WidgetShowTop399; TextEditorRunTrace "Open Window Message" "b"

SNAPDelete

DeleteFile $SNAPParameterFileOutput
DeleteFile "$SNAPParameterFileOutput.hdr"

set SNAPParaName [file tail $SNAPParameterFileInput]; set SNAPParameterName [file rootname $SNAPParaName] 

if {$SNAPSensor == "S1A"} {
    CopyFile $SNAPLeaderFile "$TMPDir/metadata.xml"
    set SNAPLeaderFileDir [file dirname $SNAPLeaderFile]
    CopyFile "$SNAPLeaderFileDir/config.txt" "$TMPDir/config.txt"
    }
if {$SNAPSensor == "S1B"} {
    CopyFile $SNAPLeaderFile "$TMPDir/metadata.xml"
    set SNAPLeaderFileDir [file dirname $SNAPLeaderFile]
    CopyFile "$SNAPLeaderFileDir/config.txt" "$TMPDir/config.txt"
    }

CopyFile $SNAPParameterFileInput "$TMPDir/target.bin"
WaitUntilCreated "$TMPDir/target.bin"
set SNAPParameterFileInput "$TMPDir/target.bin"
EnviWriteConfig $SNAPParameterFileInput $NligFullSize $NcolFullSize 4
WaitUntilCreated "$TMPDir/target.bin.hdr"

TextEditorRunTrace "Process The Function Soft/bin/tools/SNAP_batch_config_file.exe" "k"
TextEditorRunTrace "Arguments: -ss $SNAPSensor -ob \x22$TMPSNAPBatchCfg\x22 -od \x22$TMPDir\x22 -ilf \x22$SNAPLeaderFile\x22 -img $SNAPResamplingIMG -dms $SNAPDEM -dmr $SNAPResamplingDEM -sdm $SNAPSaveDEM -sia $SNAPSaveIncAng -spi $SNAPSaveProjIncAng -rad $SNAPRadioCorrec -pix $SNAPPixelSize -ipf \x22$SNAPParameterFileInput\x22 -ipn $SNAPParaName -mrgi $SNAPMlkRgIn -mazi $SNAPMlkAzIn -mrgo $SNAPMlkRgOut -mazo $SNAPMlkAzOut -dmf \x22$SNAPDEMFile\x22" "k"
set f [ open "| Soft/bin/tools/SNAP_batch_config_file.exe -ss $SNAPSensor -ob \x22$TMPSNAPBatchCfg\x22 -od \x22$TMPDir\x22 -ilf \x22$SNAPLeaderFile\x22 -img $SNAPResamplingIMG -dms $SNAPDEM -dmr $SNAPResamplingDEM -sdm $SNAPSaveDEM -sia $SNAPSaveIncAng -spi $SNAPSaveProjIncAng -rad $SNAPRadioCorrec -pix $SNAPPixelSize -ipf \x22$SNAPParameterFileInput\x22 -ipn $SNAPParaName -mrgi $SNAPMlkRgIn -mazi $SNAPMlkAzIn -mrgo $SNAPMlkRgOut -mazo $SNAPMlkAzOut -dmf \x22$SNAPDEMFile\x22" r]
TextEditorRunTrace "Check RunTime Errors" "r"
CheckRunTimeError

WaitUntilCreated $TMPSNAPBatchCfg

if [file exists $SnapReader] {
    TextEditorRunTrace "Process The Function SNAP/gpt" "k"
    TextEditorRunTrace "Arguments: \x22$TMPSNAPBatchCfg\x22" "k"
    set f [ open "| \x22$SnapGpt\x22 \x22$TMPSNAPBatchCfg\x22" r]
    set FlagStop 0
    fconfigure $f -buffering line
    while {$FlagStop == 0} {
        if {![eof $f]} {
            gets $f tmp
            update
            } else {
            set FlagStop 1
            catch "close $f"
            }
        }
    TextEditorRunTrace "Check RunTime Errors" "r"
    CheckRunTimeError
    WidgetHideTop399; TextEditorRunTrace "Close Window Message" "b"

    set TMPConfigTxt "$TMPDir/target/config.txt"
    if [file exists $TMPConfigTxt] {
        DeleteFile $SNAPParameterFileInput
        DeleteFile "$SNAPParameterFileInput.hdr"
        DeleteFile "$SNAPDirOutput/config.txt"
        DeleteFile "$SNAPDirOutput/config_mapinfo.txt"
        if [file exists "$TMPDir/target/target.bin.bin.hdr"] { set TMPConfigHdr "$TMPDir/target/target.bin.bin.hdr" }
        if [file exists "$TMPDir/target/target.bin_HH.bin.hdr"] { set TMPConfigHdr "$TMPDir/target/target.bin_HH.bin.hdr" }
        if [file exists "$TMPDir/target/target.bin_HV.bin.hdr"] { set TMPConfigHdr "$TMPDir/target/target.bin_HV.bin.hdr" }
        if [file exists "$TMPDir/target/target.bin_VH.bin.hdr"] { set TMPConfigHdr "$TMPDir/target/target.bin_VH.bin.hdr" }
        if [file exists "$TMPDir/target/target.bin_VV.bin.hdr"] { set TMPConfigHdr "$TMPDir/target/target.bin_VV.bin.hdr" }
        TextEditorRunTrace "Process The Function Soft/bin/tools/SNAP_mapinfo_config_file.exe" "k"
        TextEditorRunTrace "Arguments: -id \x22$SNAPDirOutput\x22 -if \x22$TMPConfigHdr\x22 -ss $SNAPSensor -pp $PolarType" "k"
        set f [ open "| Soft/bin/tools/SNAP_mapinfo_config_file.exe -id \x22$SNAPDirOutput\x22 -if \x22$TMPConfigHdr\x22 -ss $SNAPSensor -pp $PolarType" r]
        TextEditorRunTrace "Check RunTime Errors" "r"
        CheckRunTimeError
        WaitUntilCreated "$SNAPDirOutput/config.txt"
        WaitUntilCreated "$SNAPDirOutput/config_mapinfo.txt"

        set TMPMetaData "$TMPDir/target/metadata.xml"
        if [file exists $TMPMetaData] {
            DeleteFile "$SNAPOutputDir/GEARTH_POLY.kml"
            TextEditorRunTrace "Process The Function Soft/bin/tools/SNAP_google_file.exe" "k"
            TextEditorRunTrace "Arguments: -od \x22$SNAPOutputDir\x22 -ifm \x22$TMPMetaData\x22 -ifh \x22$TMPConfigHdr\x22" "k"
            set f [ open "| Soft/bin/tools/SNAP_google_file.exe -od \x22$SNAPOutputDir\x22 -ifm \x22$TMPMetaData\x22 -ifh \x22$TMPConfigHdr\x22" r]
            TextEditorRunTrace "Check RunTime Errors" "r"
            CheckRunTimeError
            WaitUntilCreated "$SNAPOutputDir/GEARTH_POLY.kml"
            set TMPMetaDataOut "$SNAPDirOutput/metadata.xml"
            CopyFile $TMPMetaData $TMPMetaDataOut
            }

        set f [open "$SNAPOutputDir/config_acquisition.txt" w]
        puts $f $SNAPAntennaPass
        puts $f [expr ($SNAPTeta0 * 180.) / 3.1415926]
        puts $f $SNAPPixelSize
        puts $f $SNAPPixelSize
        close $f

        set ConfigFile "$SNAPDirOutput/config.txt"
        WaitUntilCreated $ConfigFile
        set ErrorMessage ""
        LoadConfig
        if {"$ErrorMessage" == ""} {
            if [file exists "$TMPDir/target/target.bin.bin"] { set SNAPParameterFileInput "$TMPDir/target/target.bin.bin" }
            if [file exists "$TMPDir/target/target.bin_HH.bin"] { set SNAPParameterFileInput "$TMPDir/target/target.bin_HH.bin" }
            if [file exists "$TMPDir/target/target.bin_HV.bin"] { set SNAPParameterFileInput "$TMPDir/target/target.bin_HV.bin" }
            if [file exists "$TMPDir/target/target.bin_VH.bin"] { set SNAPParameterFileInput "$TMPDir/target/target.bin_VH.bin" }
            if [file exists "$TMPDir/target/target.bin_VV.bin"] { set SNAPParameterFileInput "$TMPDir/target/target.bin_VV.bin" }
            if [file exists $SNAPParameterFileInput ] { 
