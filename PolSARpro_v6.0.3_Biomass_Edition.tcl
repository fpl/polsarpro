    puts $ftmp $numberOfRows
    puts $ftmp $numberOfColumns

    #TerraSar Polar Definition = TxRx so dual pol = (HH,HV) (VH,VV) or (HH,VV)
    set TSXPol ""; set PolType ""
    for {set i 1} {$i <= $polLayerInd} {incr i} { append TSXPol "$polLayerImg($i) " }
