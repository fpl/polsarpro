        gets $f Version
        close $f
        }
    set PSPEnv "PolSARpro-Bio_"; append PSPEnv $Version
    set temp0 $env(TMP); set temp ""
    set Lengthtemp [string length $temp0]
        for {set i 0} {$i <= $Lengthtemp} {incr i} {
            set lettre [string range $temp0 $i $i]
            if {$lettre == "\\"} { set lettre "/" }
            append temp $lettre
