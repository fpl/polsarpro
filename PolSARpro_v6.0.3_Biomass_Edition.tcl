## Procedure:  PSPFullScreenInit-S1A

proc ::PSPFullScreenInit-S1A {} {
global PSPFullScreen PSPRunTrace PlatForm

#taille initiale width = 1330 height = 100

set geoscreenwidth [winfo screenwidth .top4]
set geoscreenheight [winfo screenheight .top4]

set tx [winfo rootx .top4]
set ty [winfo rooty .top4]
set x [winfo x .top4]
set y [winfo y .top4]
set geoscreenborderw [expr {$tx-$x}]
set geoscreentitleh [expr {$ty-$y}]
if {$PlatForm == "unix"} {
    set geoscreentitleh [expr {$y-1}]
    set geoscreenborderw [expr 1 + round(($geoscreentitleh * 1.0) / 10.0)]
    }

set geowidth [expr $geoscreenwidth - (2 * $geoscreenborderw)]

set geoheight 100
if {$PSPFullScreen == 1} {
    set geoheight [expr $geoscreenheight - ($geoscreentitleh + $geoscreenborderw) ]
    if {$PSPRunTrace == 1} {
        set offsetbottom [expr $geoscreenborderw + $geoscreentitleh + 50 ] 
        set geoheight [expr $geoheight - $offsetbottom ]
        }
    }

#Progress Bar size
set PSPPB [expr $geowidth - 1330 + 150]
.top4.fra71.cpd69.pro68 configure -width $PSPPB; update

set geometrie $geowidth; append geometrie "x"; append geometrie $geoheight; append geometrie "+1+1"
wm geometry .top4 $geometrie; update
Window show .top4; TextEditorRunTrace "Open Window PolSARpro - Sentinel-1 Mission v1.0 Main Menu" "b"
}
